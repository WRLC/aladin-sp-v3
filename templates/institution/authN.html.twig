{% extends 'base.html.twig' %}
{% block body %}
    <h1>Authentication Test</h1>
    <h2>User Attributes:</h2>
    <dl>
        {% for key, value in attributes %}
            <dt>{{ key }}</dt>
            <dd>
                <ul>
                    {% for item in value %}
                        <li>{{ item }}</li>
                    {% endfor %}
                </ul>
            </dd>
        {% endfor %}
    </dl>
{% endblock %}