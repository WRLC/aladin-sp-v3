{% extends 'base.html.twig' %}

{% block body %}
    <h1>Error</h1>
    <div class="alert alert-danger" role="alert">
        <div class="mb-3">{{ intro }}</div>
        <ul>
            {% for error in errors %}
                <li><pre>{{ error }}</pre></li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}